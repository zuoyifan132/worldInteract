{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                "--input-dir",
                "data/raw_apis",
                "--output-file",
                "data/processed_apis/bfcl_collection_example/cleaned_apis.json"
            ]
        }
    ]
}