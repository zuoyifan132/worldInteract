{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                "--api-collection",
                "data/dependency_graphs/my_dependency_graphs/domains/database_operations.json"
            ]
        }
    ]
}